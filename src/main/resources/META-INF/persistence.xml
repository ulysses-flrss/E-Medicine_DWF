<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="default">

        <class>com.dwf.entities.AnunciosEntity</class>
        <class>com.dwf.entities.CitasEntity</class>
        <class>com.dwf.entities.EspecialidadesEntity</class>
        <class>com.dwf.entities.FamiliaresEntity</class>
        <class>com.dwf.entities.RolesEntity</class>
        <class>com.dwf.entities.UsuariosEntity</class>
        <properties>
            <property name="toplink.jdbc.url" value="jdbc:mysql://localhost:3306/e-medicine2?useSSL=false"/>
            <property name="toplink.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
            <property name="toplink.jdbc.user" value="root"/>
            <property name="hibernate.connection.url" value="jdbc:mysql://localhost:3306/e-medicine2?useSSL=false"/>
            <property name="hibernate.connection.driver_class" value="com.mysql.cj.jdbc.Driver"/>
            <property name="hibernate.connection.username" value="root"/>
            <property name="openjpa.ConnectionURL" value="jdbc:mysql://localhost:3306/e-medicine2?useSSL=false"/>
            <property name="openjpa.ConnectionDriverName" value="com.mysql.cj.jdbc.Driver"/>
            <property name="openjpa.ConnectionUserName" value="root"/>
            <property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/e-medicine2?useSSL=false"/>
            <property name="eclipselink.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
            <property name="eclipselink.jdbc.user" value="root"/>
        </properties>
    </persistence-unit>
</persistence>
